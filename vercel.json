{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1"
    },
    {
      "src": "/api/socketio",
      "dest": "/api/socketio.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
